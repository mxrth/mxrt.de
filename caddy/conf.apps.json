{
  "http": {
    "servers": {
      "srv0": {
        "listen": [
          ":443"
        ],
        "routes": [
          {
            "match": [
              {
                "host": [
                  "www.mxrt.de"
                ]
              }
            ],
            "handle": [
              {
                "handler": "subroute",
                "routes": [
                  {
                    "handle": [
                      {
                        "handler": "static_response",
                        "headers": {
                          "Location": [
                            "https://blog.mxrt.de"
                          ]
                        },
                        "status_code": 302
                      }
                    ]
                  }
                ]
              }
            ],
            "terminal": true
          },
          {
